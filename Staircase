import math
import os
import random
import re
import sys

# Complete the staircase function below.
def staircase(n):
    for a in range(1, n+1):
        """This is where the real work is done
        rjust() is a string method"""
        # As such don't forget to treat it as a SINGLE string
        # by using ()
        print((a * "#").rjust(n))
    
    
if __name__ == '__main__':
    n = int(input())

    staircase(n)
